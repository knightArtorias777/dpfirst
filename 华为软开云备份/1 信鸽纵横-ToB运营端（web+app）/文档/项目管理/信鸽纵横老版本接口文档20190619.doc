<pre><code class="language-java">    /**
     * @Description  导入
     * @Author hcj
     * @CreateTime 2019-02-28 16:33
     * @Param [file]
     * @return io.lbz.common.utils.R
     **/
    @PostMapping("/excel/import")
    public R excelImport(@RequestParam("file") MultipartFile file) throws Exception {
        String excelTemplateMapping = "excel_template/test_xls/sysDept_mapping.xml";

        if (file.isEmpty()) {
            return R.error("文件不存在");
        }

        InputStream ins = file.getInputStream();
        // 解析模板并导入到数据库里,使用jxls reader读取excel数据
        InputStream inputXML = Thread.currentThread().getContextClassLoader()
                .getResourceAsStream(excelTemplateMapping);
        XLSReader mainReader = ReaderBuilder.buildFromXML(inputXML);
        InputStream inputXLS = ins;
        List<TestSysDeptXls> dicts = new ArrayList<TestSysDeptXls>();
        Map beans = new HashMap();
        beans.put("list", dicts);
        ReaderConfig.getInstance().setSkipErrors(true);
        XLSReadStatus readStatus = mainReader.read(inputXLS, beans);
        List<XLSReadMessage> errors = readStatus.getReadMessages();

        StringBuilder sb = JxlsUtils.getXlsError(errors, new String[]{"E"}, false);//E列的数据可以选填
        if (sb != null) return R.error("excel解析错误！" + sb.toString());
        ins.close();

        // TODO 字段必填的校验
        //return R.error("excel数据未正确填写！<br>"+sb.toString());

        try {
            //  TODO 批量插入,异步处理
            dicts.stream().forEach(x -> log.info("{}", x));
            return R.ok("导入数据成功");
        } catch (Exception ex) {
            ex.printStackTrace();
            return R.error(ex.getMessage());
        }
    }

    /**
     * @Description  导出
     * @Author hcj
     * @CreateTime 2019-02-28 16:33
     * @Param [response, condtion]
     * @return io.lbz.common.utils.R
     **/
    @PostMapping("/excel/export")
    public void excelExport(HttpServletResponse response, @RequestBody JSONObject dto) {
        String excelTemplate = "excel_template/test_xls/sysDept_collection_template.xls";
        // /excel_template/test_xls/sysDept_collection_template.xls
        // TODO 根据条件，从数据库中查询数据
        log.info("dto is :{}", JSONObject.toJSONString(dto));
        // map封装的数据
        List<Map> excelList = new ArrayList<>();
        Map dept_1 = new HashMap();
        dept_1.put("id", "1");
        dept_1.put("deptId", "123456789123456789");//long类型的要转成string,防止丢精度
        dept_1.put("name", "name_one");
        dept_1.put("parentId", 1L);
        dept_1.put("orderNum", 1);
        dept_1.put("delFlag", -1);
        excelList.add(dept_1);

        // 实体封装的数据
        List<TestSysDeptXls> dataList = new ArrayList<>();
        TestSysDeptXls dept = new TestSysDeptXls();
        dept.setId(1);
        dept.setDeptId("123456789123456789");
        dept.setName("哈哈");
        dept.setParentId("1");
        dept.setOrderNum(1);
        dept.setDelFlag(-1);
        dataList.add(dept);

        // 设置被下载而不是被打开
        response.setContentType("application/gorce-download");
        response.setHeader("content-Type", "application/vnd.ms-excel");
        // 设置被第三方工具打开,设置下载的文件名
        response.addHeader("Content-disposition", "attachment;fileName=" + DateUtil.format(new Date(), "yyyyMMdd-HHmmss") + ".xls");
        try (InputStream is = Thread.currentThread().getContextClassLoader().getResourceAsStream(excelTemplate)) {
            if (is == null) {
                throw new RRException("模板资源不存在：" + excelTemplate);
            }

            final File tempFile = File.createTempFile("temp", ".xls");//创建临时文件
            logger.info("临时文件所在的本地路径：" + tempFile.getCanonicalPath());
            OutputStream out = new FileOutputStream(tempFile);
            Map<String, Object> map = new HashMap<>();
            map.put("list", excelList);
            JxlsUtils.exportExcel(is, out, map);
            out.close();

            // 下载:把有数据的临时文件发送给brower
            try (InputStream fileInputStream = new FileInputStream(tempFile)) {
                OutputStream os = response.getOutputStream();
                byte[] bytes = new byte[1024];
                int len = 0;
                while ((len = fileInputStream.read(bytes)) != -1) {
                    os.write(bytes, 0, len);
                }
                os.flush();
                os.close();
            } catch (IOException e) {
                throw new RRException(e.getMessage());
            }
        } catch (IOException e) {
            throw new RRException(e.getMessage());
        }

    }</code></pre></table>
