@PostMapping("/online")
@RequiresPermissions("xg:shed:online")
public R online(@RequestBody CommonDto dto) {
//设置公棚默认可见：1
int status = 1;
if (StringUtils.isNotBlank(dto.getLineType()))
status = Integer.valueOf(dto.getLineType());
List<ShedEntity> list = new ArrayList<>();
for (Long id : dto.getIds()) {
ShedEntity shedEntity = new ShedEntity();
shedEntity.setId(id);
shedEntity.setStatus(status);
list.add(shedEntity);
}
shedService.updateBatchById(list);
return R.ok();
}</code></pre></table>
